{
  "name": "onedev-browser",
  "displayName": "oneDev Browser",
  "description": "Browse issues, PRs, and builds on oneDev repositories",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "onedev-browser.setCredentials",
        "title": "oneDev: Set Credentials"
      }
    ],
    "configuration": {
      "title": "oneDev Browser",
      "properties": {
        "onedev-browser.url": {
          "type": "string",
          "default": "",
          "description": "URL of your oneDev instance"
        },
        "onedev-browser.email": {
          "type": "string",
          "default": "",
          "description": "Your oneDev account email"
        },
        "onedev-browser.token": {
          "type": "string",
          "default": "",
          "description": "Your oneDev API token"
        },
        "onedev-browser.projectName": {
          "type": "string",
          "default": "",
          "description": "Your oneDev project name"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/node-fetch": "^2.6.11",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^2.7.0"
  }
}